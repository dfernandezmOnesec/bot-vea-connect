# Configuración de Azure
AZURE_STORAGE_CONNECTION_STRING=your_azure_storage_connection_string_here
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here

# Configuración de Redis
REDIS_CONNECTION_STRING=your_redis_connection_string_here

# Configuración de WhatsApp
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token_here
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id_here
WHATSAPP_VERIFY_TOKEN=your_whatsapp_verify_token_here

# Configuración de Computer Vision
AZURE_VISION_ENDPOINT=https://your-vision-resource.cognitiveservices.azure.com/
AZURE_VISION_API_KEY=your_vision_api_key_here

# Azure Communication Services (ACS)
ACS_ENDPOINT=https://your-acs-resource.communication.azure.com/
ACS_CHANNEL_ID=your_acs_channel_id_here
ACS_ACCESS_KEY=your_acs_access_key_here
ACS_CONNECTION_STRING=your_acs_connection_string_here

# Event Grid
EVENT_GRID_TOPIC_ENDPOINT=https://your-event-grid-topic.westus2-1.eventgrid.azure.net/api/events
EVENT_GRID_TOPIC_KEY=your_event_grid_topic_key_here
EVENT_GRID_WEBHOOK_SECRET=your_event_grid_webhook_secret_here

# Configuración de la aplicación
ENVIRONMENT=development
LOG_LEVEL=INFO
MAX_TOKENS=4000
TEMPERATURE=0.7 